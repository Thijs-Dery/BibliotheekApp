             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             Height="300" Width="400">
    <Grid Margin="0,0,0,-82">
        <StackPanel Margin="20">
            <Label Content="Auteurslijst"/>
            <ListView x:Name="AuteursListView" Height="200"/>
            <Button Content="Terug" Width="100" Click="Terug_Click" Margin="0,20,0,0" Height="23"/>
        </StackPanel>
    </Grid>
</UserControl>

